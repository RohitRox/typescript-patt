{
  "name": "master-interface",
  "version": "0.0.1",
  "main": "app/server.ts",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "rimraf dist && tsc",
    "start:dev": "cross-env AWS_SDK_LOAD_CONFIG=1 NODE_ENV=development nodemon",
    "start:debug": "cross-env NODE_ENV=development nodemon --config nodemon-debug.json",
    "start:prod": "NODE_ENV=production pm2-runtime dist/main.js",
    "lint": "tslint --project tsconfig.json",
    "test": "jest --no-cache",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "format": "prettier --write './**/*.{js,ts,json,md}'"
  },
  "dependencies": {
    "aws-sdk": "^2.473.0",
    "axios": "^0.18.0",
    "connect-memcached": "^1.0.0",
    "express": "^4.16.4",
    "express-session": "^1.16.1",
    "helmet": "^3.16.0",
    "morgan": "^1.9.1",
    "newrelic": "^5.6.4",
    "pm2": "^3.5.0",
    "sputnik-app-kit": "0.0.1-pre9",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/express-session": "^1.15.12",
    "@types/helmet": "^0.0.43",
    "@types/jest": "^24.0.11",
    "@types/morgan": "^1.7.35",
    "@types/nock": "^10.0.2",
    "@types/node": "^11.13.4",
    "@types/supertest": "^2.0.7",
    "cross-env": "^5.2.0",
    "jest": "^24.7.1",
    "nock": "^10.0.6",
    "nodemon": "^1.18.11",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.3",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.3"
  }
}
